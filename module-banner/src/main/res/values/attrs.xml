<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="BannerView">
    <attr format="enum" name="orientation">
      <enum name="horizontal" value="0"/>
      <enum name="vertical" value="1"/>
    </attr>
    <attr format="integer" name="durationTime"/>
    <attr format="boolean" name="isShowIndicator"/>
    <attr format="boolean" name="isAutoPlay"/>
    <attr format="boolean" name="isLoop"/>
    <attr format="boolean" name="isScaleCover"/>
    <attr format="float" name="scaleSize"/>
    <attr format="boolean" name="isShowBothEnds"/>
    <attr format="dimension" name="BothEndsSpace"/>
    <attr format="boolean" name="isInitOffsetCenter"/>

  </declare-styleable>
</resources>